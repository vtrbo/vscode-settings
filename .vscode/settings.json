{
  // Visuals
  // 控制工作台中活动栏的可见性
  "workbench.activityBar.visible": true,
  // 指定用在工作台中的颜色主题
  "workbench.colorTheme": "Vitesse Dark",
  // 控制编辑器的选项卡关闭按钮的位置
  "workbench.editor.tabCloseButton": "left",
  // 控制在工作台中字体的渲染方式
  "workbench.fontAliasing": "antialiased",
  // 指定工作台中使用的文件图标主题
  "workbench.iconTheme": "file-icons",
  // 控制列表和树是否具有平滑滚动效果
  "workbench.list.smoothScrolling": true,
  // 指定深色操作系统外观的首选颜色主题
  "workbench.preferredDarkColorTheme": "Vitesse Dark",
  // 指定浅色操作系统外观的首选颜色主题
  "workbench.preferredLightColorTheme": "Vitesse Light",
  // 指定使用的产品图标主题
  "workbench.productIconTheme": "icons-carbon",
  // 控制主边栏和活动栏的位置
  "workbench.sideBar.location": "right",
  // 控制启动时显示的编辑器
  "workbench.startupEditor": "newUntitledFile",
  // 控制在单击文件夹名称时如何扩展树文件夹
  "workbench.tree.expandMode": "singleClick",
  // 控制树缩进
  "workbench.tree.indent": 10,
  // 控制字体系列
  "editor.fontFamily": "Input Mono, Fira Code, monospace",
  // 控制字体变体
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss06', 'zero'",
  // 控制字体大小(像素)
  "editor.fontSize": 13.5,
  // 控制编辑器是否应呈现垂直字形边距
  "editor.glyphMargin": true,
  // 控制折行的方式
  "editor.wordWrap": "on",
  // Editor
  // 控制 UI 是否应在已针对屏幕阅读器进行优化的模式下运行
  "editor.accessibilitySupport": "off",
  // 控制是否启用平滑插入动画
  "editor.cursorSmoothCaretAnimation": "on",
  // 控制 "查找小部件" 是否应在编辑器顶部添加额外的行
  "editor.find.addExtraSpaceOnTop": false,
  // 控制是否启用括号对指南
  "editor.guides.bracketPairs": "active",
  // 控制是否在编辑器中自动显示内联建议
  "editor.inlineSuggest.enabled": true,
  // 控制行号的显示
  "editor.lineNumbers": "interval",
  // 用于使用鼠标添加多个游标的修饰符
  "editor.multiCursorModifier": "alt",
  // 控制编辑器在空白字符上显示符号的方式
  "editor.renderWhitespace": "boundary",
  // 控制在建议列表中如何预先选择建议
  "editor.suggestSelection": "first",
  // 一个制表符等于的空格数
  "editor.tabSize": 2,
  // 控制是否突出显示仅保留空格或完全没有宽度的字符
  "editor.unicodeHighlight.invisibleCharacters": false,
  // 在编辑器顶部的滚动过程中显示嵌套的当前作用域
  "editor.stickyScroll.enabled": true,
  // 控制当鼠标移动到悬停提示上时，其是否保持可见
  "editor.hover.sticky": true,
  // 要在保存时运行的代码操作种类
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true, // this allows ESLint to auto fix on save
    "source.organizeImports": false
  },
  // 执行单词相关的导航或操作时作为单词分隔符的字符
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  // 控制资源管理器是否应在通过回收站删除文件时要求确认
  "explorer.confirmDelete": false,
  // 控制资源管理器是否应在通过拖放移动文件或文件夹时要求确认
  "explorer.confirmDragAndDrop": false,
  // 根据 OS 外观自动切换到首选颜色主题
  "window.autoDetectColorScheme": false,
  // 调整对话框窗口的外观
  "window.dialogStyle": "custom",
  // 启用 macOS Sierra 窗口选项卡
  "window.nativeTabs": true, // this is great, macOS only
  // 调整窗口标题栏的外观
  "window.titleBarStyle": "custom",
  // 控制在会话期间显示已打开文件的编辑器是否应在被其他进程删除或重命名时自动关闭
  "workbench.editor.closeOnFileDelete": true,
  // 控制是否在具有未保存更改的编辑器的选项卡上绘制顶部边框
  "workbench.editor.highlightModifiedTabs": true,
  // 控制打开的编辑器数是否应受限制
  "workbench.editor.limit.enabled": true,
  // 控制最大打开的编辑器的限制是否应应用于每个编辑器组或所有编辑器组
  "workbench.editor.limit.perEditorGroup": true,
  // 控制打开编辑器的最大数量
  "workbench.editor.limit.value": 5,
  // 控制扩展的自动更新行为
  "extensions.autoUpdate": "onlyEnabledExtensions",
  // 启用后，将不会显示扩展建议的通知
  "extensions.ignoreRecommendations": true,
  // 默认行尾字符
  "files.eol": "\n",
  // 启用后，保存文件时在文件末尾插入一个最终新行
  "files.insertFinalNewline": true,
  // 启用简单文件对话框以打开和保存文件和文件夹
  "files.simpleDialog.enable": true,
  // 若设置为 true，则自动从当前 Git 仓库的默认远程仓库提取提交
  "git.autofetch": true,
  // 同步 Git 存储库前请先进行确认
  "git.confirmSync": false,
  // 在没有暂存的更改时提交所有更改
  "git.enableSmartCommit": true,
  // 控制未跟踪的更改的行为
  "git.untrackedChanges": "separate",
  // 控制终端光标是否闪烁
  "terminal.integrated.cursorBlinking": true,
  // 控制终端光标的样式
  "terminal.integrated.cursorStyle": "line",
  // 要在终端中用于非粗体文本的字体粗细
  "terminal.integrated.fontWeight": "300",
  // 当必须关闭终端进程(例如当窗口或应用程序关闭时)时，这将决定下次打开工作区时，应在何时还原以前的终端会话内容/历史记录和重新创建进程
  "terminal.integrated.persistentSessionReviveProcess": "never",
  // 控制终端选项卡是否以列表的形式显示在终端的一侧
  "terminal.integrated.tabs.enabled": true,
  // 控制装订线中差异修饰的宽度(px)(已添加或已修改)
  "scm.diffDecorationsGutterWidth": 2,
  // 控制在运行预启动任务后遇到错误时应该怎么做
  "debug.onTaskErrors": "debugAnyway",
  // 启用后，差异编辑器将忽略前导空格或尾随空格中的更改
  "diffEditor.ignoreTrimWhitespace": false,
  // 配置 glob 模式以在全文搜索和快速打开中排除文件和文件夹
  "search.exclude": {
    "**/~": true,
    "**/.git": true,
    "**/.github": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
  },
  // Extension configs
  // 若为 true，Emmet 建议将显示为代码片段
  "emmet.showSuggestionsAsSnippets": true,
  // 启用后，在按 Tab 时会展开 Emmet 缩写，即使未显示完成
  "emmet.triggerExpansionOnTab": false,
  // 自定义要突出显示的诊断级别(严重性)
  "errorLens.enabledDiagnosticLevels": [
    "warning",
    "error"
  ],
  // 指定不应突出显示的源或源(代码)对
  "errorLens.excludeBySource": [
    "cSpell",
    "Grammarly",
    "eslint"
  ],
  // ESLint config: https://github.com/antfu/eslint-config
  // 在快速修复菜单中显示打开的lint规则文档网页
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  // 打开安静模式，忽略警告
  "eslint.quiet": true,
  // 一个由ESLint验证的语言数组
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "svelte",
    "json",
    "jsonc",
    "json5"
  ],
  // 启用/禁用复合词
  "cSpell.allowCompoundWords": true,
  // 当前活动拼写语言
  "cSpell.language": "en,en-US",
  // 十六进制颜色必须由三个或六个十六进制数字组成
  "css.lint.hexColorLength": "ignore",
  // 指定是否提供作者CodeLens，显示文件或代码块的作者数量以及最突出的作者(如果有多个)
  "gitlens.codeLens.authors.enabled": false,
  // 指定是否在默认情况下提供任何Git CodeLens
  "gitlens.codeLens.enabled": false,
  // 指定是否提供最近的更改CodeLens，显示文件或代码块最近提交的作者和日期
  "gitlens.codeLens.recentChange.enabled": false,
  // 指定将向哪些菜单添加哪些命令
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "history": false,
      "remote": false
    },
    "editorGroup": {
      "blame": true,
      "compare": false
    },
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scm": {
      "authors": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scmGroupInline": {
      "stash": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": false,
      "stash": true
    }
  },
  // 为项目启用区域自动检测
  "i18n-ally.autoDetection": false,
  // 显示语言
  "i18n-ally.displayLanguage": "en",
  // 隐藏的语言
  "i18n-ally.ignoredLocales": [],
  // 启用图标化内联注释
  "iconify.annotations": true,
  // 使用图标图形代替图标名称
  "iconify.inplace": false,
  // I only use Prettier for manually formatting
  // 启用 Prettier
  "prettier.enable": false,
  // 每行代码的长度限制
  "prettier.printWidth": 200,
  // 在所有代码语句的末尾添加分号
  "prettier.semi": false,
  // 使用单引号而不是双引号
  "prettier.singleQuote": true,
  // File Nesting
  // this might not be up to date with the repo, please check yourself
  // https://github.com/antfu/vscode-file-nesting-config
  // 自动更新的最小间隔，以分钟为单位
  "fileNestingUpdater.autoUpdateInterval": 360,
  // 在进行自动更新之前应该显示提示吗
  "fileNestingUpdater.promptOnAutoUpdate": false,
  // 你想要更新的上游仓库
  "fileNestingUpdater.upstreamRepo": "antfu/vscode-file-nesting-config",
  // 上游仓库的分支
  "fileNestingUpdater.upstreamBranch": "main",
  // 控制是否已在资源管理器中启用文件嵌套
  "explorer.fileNesting.enabled": true,
  // 控制是否自动扩展文件嵌套
  "explorer.fileNesting.expand": false,
  // 控制资源管理器中的文件嵌套
  "explorer.fileNesting.patterns": {
    "*.asax": "$(capture).*.cs, $(capture).*.vb",
    "*.ascx": "$(capture).*.cs, $(capture).*.vb",
    "*.ashx": "$(capture).*.cs, $(capture).*.vb",
    "*.aspx": "$(capture).*.cs, $(capture).*.vb",
    "*.bloc.dart": "$(capture).event.dart, $(capture).state.dart",
    "*.c": "$(capture).h",
    "*.cc": "$(capture).hpp, $(capture).h, $(capture).hxx",
    "*.cjs": "$(capture).cjs.map, $(capture).*.cjs, $(capture)_*.cjs",
    "*.component.ts": "$(capture).component.html, $(capture).component.spec.ts, $(capture).component.css, $(capture).component.scss, $(capture).component.sass, $(capture).component.less",
    "*.cpp": "$(capture).hpp, $(capture).h, $(capture).hxx",
    "*.cs": "$(capture).*.cs",
    "*.cshtml": "$(capture).cshtml.cs",
    "*.csproj": "*.config, *proj.user, appsettings.*, bundleconfig.json",
    "*.css": "$(capture).css.map, $(capture).*.css",
    "*.cxx": "$(capture).hpp, $(capture).h, $(capture).hxx",
    "*.dart": "$(capture).freezed.dart, $(capture).g.dart",
    "*.ex": "$(capture).html.eex, $(capture).html.heex, $(capture).html.leex",
    "*.go": "$(capture)_test.go",
    "*.java": "$(capture).class",
    "*.js": "$(capture).js.map, $(capture).*.js, $(capture)_*.js",
    "*.jsx": "$(capture).js, $(capture).*.jsx, $(capture)_*.js, $(capture)_*.jsx",
    "*.master": "$(capture).*.cs, $(capture).*.vb",
    "*.mjs": "$(capture).mjs.map, $(capture).*.mjs, $(capture)_*.mjs",
    "*.module.ts": "$(capture).resolver.ts, $(capture).controller.ts, $(capture).service.ts",
    "*.pubxml": "$(capture).pubxml.user",
    "*.resx": "$(capture).*.resx, $(capture).designer.cs, $(capture).designer.vb",
    "*.tex": "$(capture).acn, $(capture).acr, $(capture).alg, $(capture).aux, $(capture).bbl, $(capture).blg, $(capture).fdb_latexmk, $(capture).fls, $(capture).glg, $(capture).glo, $(capture).gls, $(capture).idx, $(capture).ind, $(capture).ist, $(capture).lof, $(capture).log, $(capture).lot, $(capture).out, $(capture).pdf, $(capture).synctex.gz, $(capture).toc, $(capture).xdv",
    "*.ts": "$(capture).js, $(capture).d.ts.map, $(capture).*.ts, $(capture)_*.js, $(capture)_*.ts",
    "*.tsx": "$(capture).ts, $(capture).*.tsx, $(capture)_*.ts, $(capture)_*.tsx",
    "*.vbproj": "*.config, *proj.user, appsettings.*, bundleconfig.json",
    "*.vue": "$(capture).*.ts, $(capture).*.js, $(capture).story.vue",
    "*.xaml": "$(capture).xaml.cs",
    "+layout.svelte": "+layout.ts,+layout.ts,+layout.js,+layout.server.ts,+layout.server.js,+layout.gql",
    "+page.svelte": "+page.server.ts,+page.server.js,+page.ts,+page.js,+page.gql",
