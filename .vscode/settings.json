{
  // 编辑器不对屏幕阅读器进行优化
  "editor.accessibilitySupport": "off",
  // 启动平滑插入动画
  "editor.cursorSmoothCaretAnimation": "on",
  // 关闭查找时顶部插入行
  "editor.find.addExtraSpaceOnTop": false,
  // 编辑器字体
  "editor.fontFamily": "Input Mono, Fira Code, monospace",
  // 开启部分连字
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss06', 'zero'",
  // 字体大小
  "editor.fontSize": 13.5,
  // 垂直字形边距
  "editor.glyphMargin": true,
  // 在活动括号对儿显示参考线
  "editor.guides.bracketPairs": "active",
  // 显示内联建议
  "editor.inlineSuggest.enabled": true,
  // 每10行显示行标
  "editor.lineNumbers": "interval",
  // 按住ctrl或Cmd键点击定义多光标
  "editor.multiCursorModifier": "alt",
  // 空白字符显示空格
  "editor.renderWhitespace": "boundary",
  // 始终选择建议列表的第一个
  "editor.suggestSelection": "first",
  // Tab的宽度
  "editor.tabSize": 2,
  // 不保留没有宽度的字符
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  // 自动换行
  "editor.wordWrap": "on",
  // 文件移到废纸篓不用询问
  "explorer.confirmDelete": false,
  // 拖动文件或文件夹不用询问
  "explorer.confirmDragAndDrop": false,
  // 仅自动更新已启用的扩展
  "extensions.autoUpdate": "onlyEnabledExtensions",
  // 不显示扩展建议
  "extensions.ignoreRecommendations": true,
  // 根据OS外观自动挂载主题
  "window.autoDetectColorScheme": false,
  // 自定义对话框的外观
  "window.dialogStyle": "custom",
  // mac多选项卡展示
  "window.nativeTabs": true,
  // 自定义标题栏的外观
  "window.titleBarStyle": "custom",
  // 编辑器活动栏可见
  "workbench.activityBar.visible": true,
  // 编辑器主题
  "workbench.colorTheme": "Vitesse Dark Soft",
  // 启动文件在编辑时外部更改的关联
  "workbench.editor.closeOnFileDelete": true,
  // 文件未保存时绘制顶部边框
  "workbench.editor.highlightModifiedTabs": true,
  // 选项卡关闭按钮在左侧显示
  "workbench.editor.tabCloseButton": "left",
  // 控制打开的编辑器数
  "workbench.editor.limit.enabled": true,
  // 控制打开编辑器数应用于编辑器组
  "workbench.editor.limit.perEditorGroup": true,
  // 最多打开10个编辑器
  "workbench.editor.limit.value": 10,
  // mac字体进行像素渲染
  "workbench.fontAliasing": "antialiased",
  // 文件图标主题
  "workbench.iconTheme": "file-icons",
  // 启动列表和树的平滑滚动效果
  "workbench.list.smoothScrolling": true,
  // 深色系统外观首选主题
  "workbench.preferredDarkColorTheme": "Vitesse Dark",
  // 浅色系统外观首选主题
  "workbench.preferredLightColorTheme": "Vitesse Light",
  // 产品图标主题
  "workbench.productIconTheme": "icons-carbon",
  // 主边栏和活动栏在右侧显示
  "workbench.sideBar.location": "right",
  // 打开新的无标题文件
  "workbench.startupEditor": "newUntitledFile",
  // 单击控制树的扩展
  "workbench.tree.expandMode": "singleClick",
  // 树的层级缩进为10
  "workbench.tree.indent": 10,
  // 行尾字符lf
  "files.eol": "\n",
  // 文件末尾保存时插入新行
  "files.insertFinalNewline": true,
  // 启用简单文件对话框
  "files.simpleDialog.enable": true,
  // 自动从远程库获取提交
  "git.autofetch": true,
  // 同步git前不用询问
  "git.confirmSync": false,
  // 没有暂存文件时提交所有更改
  "git.enableSmartCommit": true,
  // 未跟踪的更改单独显示在源代码管理中
  "git.untrackedChanges": "separate",
  // 终端光标闪烁
  "terminal.integrated.cursorBlinking": true,
  // 终端光标竖线
  "terminal.integrated.cursorStyle": "line",
  // 终端字体粗细300
  "terminal.integrated.fontWeight": "300",
  // 不还原终端缓冲区或重新创建流程
  "terminal.integrated.persistentSessionReviveProcess": "never",
  // 终端选项卡列表显示
  "terminal.integrated.tabs.enabled": true,
  // 装订线差异修饰宽度2px
  "scm.diffDecorationsGutterWidth": 2,
  // 预启动时忽略任务错误并开始调试
  "debug.onTaskErrors": "debugAnyway",
  // 关闭忽略前导空格和尾随空格的更改
  "diffEditor.ignoreTrimWhitespace": false,
  // 全局搜索排除文件或文件夹
  "search.exclude": {
    "**/~": true,
    "**/.git": true,
    "**/.github": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/.history/**": true
  },
  // 显示Emmet建议
  "emmet.showSuggestionsAsSnippets": true,
  // 关闭Tab展开Emmet缩写
  "emmet.triggerExpansionOnTab": false,
  // 警告和错误时突出显示
  "errorLens.enabledDiagnosticLevels": ["warning", "error"],
  // 不突出显示校验源
  "errorLens.excludeBySource": ["cSpell", "Grammarly", "eslint"],
  // 在快速修复中显示开放Eslint规则网页
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  // 开启静音模式忽略警告
  "eslint.quiet": true,
  // Eslint校验语言
  "eslint.validate": ["javascript", "typescript", "javascriptreact", "typescriptreact", "vue", "svelte", "html", "markdown", "json", "jsonc", "json5"],
  // Stylelint校验语言
  "stylelint.validate": ["html", "vue", "svelte", "css", "less", "scss", "postcss"],
  // 启用单词复合词
  "cSpell.allowCompoundWords": true,
  // 拼写采用英语和英式英语
  "cSpell.language": "en,en-US",
  // 忽略Hex颜色的长度
  "css.lint.hexColorLength": "ignore",
  // 启用区域自动检测
  "i18n-ally.autoDetection": false,
  // 显示的语言
  "i18n-ally.displayLanguage": "zh",
  // 隐藏的语言
  "i18n-ally.ignoredLocales": [],
  // 启用Iconify内联注释
  "iconify.annotations": true,
  // 使用图标图形替换图标名称
  "iconify.inplace": false,
  // 关闭prettier的自动格式化
  "prettier.enable": false,
  // 每行代码的长度为200
  "prettier.printWidth": 200,
  // 移除行尾分号
  "prettier.semi": false,
  // 使用单引号
  "prettier.singleQuote": true,
  // Vite项目关闭自动启动
  "vite.autoStart": false,
  // 自动获取和更新最新的配置
  "fileNestingUpdater.autoUpdate": true,
  // 720分钟更新一次
  "fileNestingUpdater.autoUpdateInterval": 720,
  // 自动更新之前显示提示符
  "fileNestingUpdater.promptOnAutoUpdate": false,
  // 获取更新的库
  "fileNestingUpdater.upstreamRepo": "antfu/vscode-file-nesting-config",
  // 获取更新的库的分支
  "fileNestingUpdater.upstreamBranch": "main",
